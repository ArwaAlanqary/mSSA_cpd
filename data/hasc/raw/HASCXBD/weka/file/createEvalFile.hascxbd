<model.BlockDiagram>
  <comment>just copy a text file</comment>
  <blocks>
    <model.BeanBlock>
      <name>reader</name>
      <class>jp.hasc.hasctool.core.runtime.source.LineReaderSource</class>
      <properties>
        <entry>
          <string>filePath</string>
          <string>$(inputFile.path)</string>
        </entry>
      </properties>
    </model.BeanBlock>
    <model.BeanBlock>
      <name>writer</name>
      <class>jp.hasc.hasctool.core.runtime.sink.LineWriterSink</class>
      <properties/>
    </model.BeanBlock>
    <model.BeanBlock>
      <name>evalFileCreator</name>
      <class>jp.hasc.hasctool.core.runtime.filter.weka.file.EvaluationFileCreator</class>
      <properties/>
    </model.BeanBlock>
  </blocks>
  <connections>
    <model.Connection>
      <output blockName="reader" portName="outputPort"/>
      <input blockName="evalFileCreator" portName="inputPort"/>
    </model.Connection>
    <model.Connection>
      <output blockName="evalFileCreator" portName="outputPort"/>
      <input blockName="writer" portName="inputPort"/>
    </model.Connection>
  </connections>
</model.BlockDiagram>